{
    "$schema": "./node_modules/ng-swagger-gen/ng-swagger-gen-schema.json",
    "swagger": "https://localhost:7052/swagger/v1/swagger.json",
    "output": "src/generated-api/",
    "prefix": "Api",
    "ignoreUnusedModels": true,
    "minParamsForContainer": 2,
    "sortParams": "desc",
    "defaultTag": "Api",
    "removeStaleFiles": true,
    "modelIndex": true,
    "serviceIndex": true,
    "apiModule": true,
    "enumModule": true,
    "generateExamples": false,
    "camelCase": true,
    "excludeTags": []
}
